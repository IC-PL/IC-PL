<h1>Posts#index</h1>

<br>

<% @post.limit(10).each do |post| %>
    <div class="container">
          <div class="image"></div>
          <div class="item"><%= post.item %></div>
          <div class="price">$<%= post.price %></div>
          <div class="buybutton"><%= link_to "More Info", post %></div> 
    </div>
<% end %>

